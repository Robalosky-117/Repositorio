# Fibonacci recursividad
def fibonacci_memo(n, memo={}):
    if n in memo:
        return memo[n]
    if n == 0:
        memo[n] = 0
    elif n == 1:
        memo[n] = 1
    else:
        memo[n] = fibonacci_memo(n-1, memo) + fibonacci_memo(n-2, memo)
    return memo[n]

# Calcular posiciones grandes
posiciones = [7, 21, 40, 71, 94]
for pos in posiciones:
    print(f"F({pos}) = {fibonacci_memo(pos)}")
