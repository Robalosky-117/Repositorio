Fuerza Bruta.py

import string
from itertools import product

def fuerza_bruta(contrasena):
    caracteres = string.digits  # solo dígitos 0-9
    longitud = len(contrasena)
    
    # Generar todas las combinaciones posibles
    for comb in product(caracteres, repeat=longitud):
        prueba = "".join(comb)
        if prueba == contrasena:
            print(f"Contraseña encontrada: {prueba}")
            return prueba
    print("Contraseña no encontrada")
    return None

# Ejemplo con contraseñas de 4, 8 y 10 dígitos
fuerza_bruta("1234")
fuerza_bruta("87654321")
fuerza_bruta("1000000000")



Greedt.py

def cambio(cantidad, denominaciones):
    resultado = []
    for moneda in denominaciones:
        if cantidad >= moneda:
            num = cantidad // moneda
            cantidad = cantidad - (num * moneda)
            resultado.append((moneda, num))
    return resultado

# Ejemplo
print(cambio(1000, [500, 200, 100, 50, 20, 5, 1]))
print(cambio(98, [50, 20, 3, 2]))


Insertion Sort.py

def insertion_sort(lista):
    for index in range(1, len(lista)):
        actual = lista[index]
        posicion = index
        
        while posicion > 0 and lista[posicion-1] > actual:
            lista[posicion] = lista[posicion-1]
            posicion -= 1
        lista[posicion] = actual
        print(lista)  # muestra el proceso paso a paso
    return lista

# Ejemplo
lista = [21, 10, 0, 11, 9, 24, 20, 14, 1]
print("Lista desordenada:", lista)
ordenada = insertion_sort(lista)
print("Lista ordenada:", ordenada)
